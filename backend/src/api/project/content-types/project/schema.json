        "Bright",
        "Pensive",
        "Poder",
        "Vibing",
        "Alertness",
        "Disfruta",
        "Zesty"
      ],
      "default": "Dark"
    },
    "outcomes": {
      "type": "component",
      "repeatable": true,
      "component": "content.outcomes"
    },
    "Tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::domain.domain",
      "inversedBy": "Projects"
